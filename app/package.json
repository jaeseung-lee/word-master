{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "pretty": "prettier --write .",
    "prepare": "cd .. && husky app/.husky",
    "proto": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src/generated/proto --ts_proto_opt=esModuleInterop=true --ts_proto_opt=snakeToCamel=keys --ts_proto_opt=stringEnums=false --ts_proto_opt=useNumericEnumForJson=true --proto_path=../proto ../proto/*.proto"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "bcryptjs": "^3.0.3",
