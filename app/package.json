    "framer-motion": "^12.34.0",
    "heic-convert": "^2.1.0",
    "jose": "^6.1.3",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-intl": "^4.8.2",
    "openai": "^6.21.0",
    "pg": "^8.18.0",
    "prisma": "^7.3.0",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "ts-proto": "^2.11.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
